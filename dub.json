{
    "name": "protobuf",
    "description": "D runtime library for Google Protocol Buffers",
    "authors": ["Drago≈ü Carp"],
    "license": "https://github.com/dcarp/protobuf-d/blob/master/LICENSE.md",
    "targetType": "library",
    "targetPath": "build",
    "workingDirectory": "build",
    "configurations": [
        {
            "name": "protobuf",
            "targetType": "library"
        },
        {
            "name": "unittest",
            "targetType": "executable",
            "dflags": ["-main"],
            "sourcePaths": ["test"],
            "mainSourceFile": "test/basic.d"
        }
    ],
    "subPackages": [
        {
            "name": "protoc-gen-d",
            "targetType": "executable",
            "targetName": "protoc-gen-d",
            "sourcePaths": ["protoc_gen_d"],
            "mainSourceFile": "protoc_gen_d/protoc-gen-d.d",
            "dependencies": {
                "protobuf": "*"
            }
         }
    ]
}
